{
  "name": "server-json",
  "version": "0.1.0",
  "description": "JSON server for my-games",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --ignore-path ../.gitignore --write \"src/**/*.{json,ts}\"",
    "lint": "eslint --ignore-path ../.gitignore --fix --ext .ts .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node src/server.ts",
    "start:watch": "nodemon --exec ts-node src/server.ts"
  },
  "author": "Kendall Roth <kendall@kendallroth.ca>",
  "license": "MIT",
  "dependencies": {
    "dayjs": "^1.11.7",
    "json-server": "^0.17.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/json-server": "^0.14.4",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
